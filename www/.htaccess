<IfModule mod_rewrite.c>
RewriteEngine On

RewriteRule docs/wiki$  /Home [R]
RewriteRule docs/wiki/$ /Home [R]

RewriteCond %{HTTPS} !=on
RewriteRule .* https://%{HTTP_HOST}%{REQUEST_URI} [R,L]
</IfModule>


<IfModule mod_alias.c>
Redirect /docs/wiki$  https://wiki.rusefi.com
Redirect /docs/wiki/$  https://wiki.rusefi.com
</IfModule>
